		"@typescript-eslint/parser": "8.12.2",
		"eslint": "9.13.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"globals": "^15.9.0",
		"prettier": "3.3.3",
