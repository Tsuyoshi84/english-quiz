		"svelte": "4.2.19",
		"svelte-check": "4.0.4",
		"svelte-preprocess": "6.0.3",
		"typescript": "5.6.2",
		"typescript-eslint": "^8.8.0",
		"vite": "5.4.10",
		"vitest": "2.0.5"
