{
  "name": "english-quiz",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --port 4100",
    "lint": "eslint src/**/*.{ts,svelte}",
    "lint:css": "stylelint **/*.{css,svelte}",
    "lint:css:fix": "stylelint **/*.{css,svelte} --fix",
    "format": "prettier --write --plugin-search-dir=. ./**/*.svelte",
    "format:check": "prettier --list-different --plugin-search-dir=. ./**/*.svelte"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "24.0.1",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-typescript": "10.0.1",
    "@tsconfig/svelte": "3.0.0",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "eslint": "8.33.0",
    "eslint-plugin-svelte3": "4.0.0",
    "postcss": "8.4.21",
    "postcss-html": "1.5.0",
    "postcss-nesting": "10.2.0",
    "prettier": "2.8.3",
    "prettier-plugin-svelte": "2.9.0",
    "rollup": "2.79.1",
    "rollup-plugin-livereload": "2.0.5",
    "rollup-plugin-svelte": "7.1.0",
    "rollup-plugin-terser": "7.0.2",
    "stylelint": "14.16.1",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-standard": "26.0.0",
    "stylelint-prettier": "2.0.0",
    "svelte": "3.55.1",
    "svelte-preprocess": "4.10.7",
    "tslib": "2.5.0",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "open-props": "1.5.5",
    "rollup-plugin-css-only": "3.1.0",
    "sirv-cli": "2.0.2"
  }
}
