		"svelte": "4.2.12",
		"svelte-check": "3.6.9",
		"svelte-preprocess": "5.1.3",
		"typescript": "5.3.3",
		"typescript-eslint": "^7.8.0",
		"vite": "5.2.10",
		"vitest": "1.6.0"
