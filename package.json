		"@sveltejs/kit": "2.5.5",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@typescript-eslint/eslint-plugin": "7.8.0",
		"@typescript-eslint/parser": "7.8.0",
		"eslint": "9.1.1",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "^2.38.0",
		"globals": "^15.1.0",
		"prettier": "3.2.5",
