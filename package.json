		"eslint": "8.56.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte3": "4.0.0",
		"prettier": "3.1.1",
		"prettier-plugin-svelte": "3.1.2",
		"stylelint": "^16.2.0",
